var rv_modal = $('#ajax-modal');
rv_modal.html('<%= j render("sessions/form") %>').modal();

// Focus on password if email is present; setTime out to allow ajax to load.
setTimeout(function () {
  var email = rv_modal.find('input#email');
    if (email.val().length > 0) {
        rv_modal.find('input#password').focus();
    } else {
      email.focus();
    }
}, 500);